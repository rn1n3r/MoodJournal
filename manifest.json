{
  "manifest_version": 2,

  "name": "Mood Journal",
  "description": "Mood Journal for passive trackinig and logging of user mood.",
  "version": "0.1",
  "options_page": "options.html",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "history",
   "alarms"
 ],
 "background": {
   "scripts": ["eventPage.js"],
   "persistent": false
 }

}
